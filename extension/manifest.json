{
  "manifest_version": 2,

  "name": "GalacticHorse",
  "description": "Un moteur de recherche enrichi dédié au handicap",
  "version": "0.0.1",

  "browser_action": {
    "default_icon" : "img/logo.png",
    "default_popup": "popup.html",
    "default_title" : "Galactic Horse"
  },
  "options_page" : "options.html",
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
